{
  "name": "match.audio",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "./node_modules/mocha/bin/mocha test/**/*.js --timeout=10000",
    "build": "browserify ./views/app.jsx | uglifyjs -cm 2>/dev/null > ./public/javascript/bundle.js",
    "clean": "rm -f ./public/javascript/bundle.js"
  },
  "engines": {
    "node": "0.12.x"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "harmony": true
        }
      ]
    ]
  },
  "dependencies": {
    "bluebird": "^2.4.2",
    "body-parser": "~1.10.0",
    "browserify": "^7.0.0",
    "compression": "^1.2.2",
    "connect-browserify": "^3.2.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.3",
    "crypto-js": "^3.1.2-5",
    "debug": "~2.1.0",
    "envify": "^3.2.0",
    "express": "~4.10.6",
    "express-session": "^1.9.2",
    "helmet": "^0.5.2",
    "moment": "^2.9.0",
    "morgan": "~1.5.0",
    "node-jsx": "^0.12.4",
    "node-uuid": "^1.4.2",
    "playmusic": "^2.0.0",
    "promised-mongo": "^0.11.1",
    "rdio": "^1.5.2",
    "react": "^0.12.1",
    "react-google-analytics": "^0.2.0",
    "react-router": "^0.11.6",
    "reactify": "^0.17.1",
    "serve-favicon": "~2.2.0",
    "spotify": "^0.3.0",
    "superagent": "^0.21.0",
    "superagent-bluebird-promise": "^0.5.1",
    "uglify-js": "^2.4.16"
  },
  "devDependencies": {
    "should": "^4.4.1",
    "mocha": "^2.0.1"
  }
}
